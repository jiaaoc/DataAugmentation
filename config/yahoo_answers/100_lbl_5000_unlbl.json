{
    "model": "BertTextClassification",
    "dataset": "yahoo_answers",
    "datapath": "processed_data/yahoo_answers_csv",
    "pretrained_weight": "bert-base-uncased",
    "is_classification": true,
    "n_labeled_per_class": 100,
    "unlabeled_per_class": 5000,
    "sharp_temperature": 0.5,
    "lambda_u": 1,
    "batch_size": 4,
    "batch_size_u": 12,
    "test_batch_size": 128,
    "epochs": 25,
    "val_iteration": 800,
    "grad_accumulation_factor": 4,
    "lr": 2e-5,
    "transform_type": "RandomInsertion"
}